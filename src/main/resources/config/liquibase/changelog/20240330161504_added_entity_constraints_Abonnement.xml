<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">
    <!--
        Added the constraints for entity Abonnement.
    -->
    <changeSet id="20240330161504-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="structure_id"
                                 baseTableName="abonnement"
                                 constraintName="fk_abonnement__structure_id"
                                 referencedColumnNames="id"
                                 referencedTableName="structure"
                                 />

        <addForeignKeyConstraint baseColumnNames="plans_abonnement_id"
                                 baseTableName="abonnement"
                                 constraintName="fk_abonnement__plans_abonnement_id"
                                 referencedColumnNames="id"
                                 referencedTableName="plans_abonnement"
                                 />

        <addForeignKeyConstraint baseColumnNames="paiement_id"
                                 baseTableName="abonnement"
                                 constraintName="fk_abonnement__paiement_id"
                                 referencedColumnNames="id"
                                 referencedTableName="paiement"
                                 />
    </changeSet>
</databaseChangeLog>
